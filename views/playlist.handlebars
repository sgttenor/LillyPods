<div id="playlist">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h1>LilyPods Playlist</h1>
        <img src="https://i.imgur.com/Tq6DSll.jpg" class="rounded" id="frog" height=90px width=105px>
        <div id="podCast">

          {{#each podcasts}}
          {{/each}}


          <h1>{{title}}</h1>
          <h2>{{id}}</h2>
          <img src="{{imageurl}}">

          {{#each podcasts}}
          {{/each}}
        </div>




      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="../public/js/index.js"></script>